## Google chrome - Dowload the google-chrome-stable_current_amd64.deb fiel

sudo dpkg -i google-chrome-stable_current_amd64.deb 
sudo apt-get -f install 
sudo dpkg -i google-chrome-stable_current_amd64.deb 

## Sublime Text 3
sudo add-apt-repository ppa:webupd8team/sublime-text-3
sudo apt-get  update
sudo apt-get install sublime-text-installer


## Popcorn Time
mkdir popcorn && cd popcorn

wget https://get.popcorntime.sh/build/Popcorn-Time-0.3.10-Linux-64.tar.xz
tar xf Popcorn-Time-0.3.10-Linux-64.tar.xz

wget https://raw.githubusercontent.com/popcorn-official/popcorn-desktop/development/Create-Desktop-Entry
chmod +x Create-Desktop-Entry
./Create-Desktop-Entry

rm Popcorn-Time-0.3.10-Linux-64.tar.xz
rm Create-Desktop-Entry

## IntelliJ Idea - Dowload the program and extract
cd /bin
./idea.sh

## TeamSpeak 3 -- Download de run file and make it executable

sudo ./file.run

# Oh My Zsh
sh -c "$(curl -fsSL https://raw.github.com/robbyrussell/oh-my-zsh/master/tools/install.sh)"

